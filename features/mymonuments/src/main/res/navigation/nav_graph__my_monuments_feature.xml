<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph__my_monuments_feature"
    app:startDestination="@id/myMonumentsFragment">

    <fragment
        android:id="@+id/myMonumentsFragment"
        android:name="com.example.practica5.mymonuments.ui.fragment.MyMonumentsFragment"
        android:label="fragment_mymonuments"
        tools:layout="@layout/fragment_mymonuments" />

    <action
        android:id="@+id/action_myMonumentsFragment_to_newMonumentFragment"
        app:destination="@id/nav_graph__new_monument_feature" />

    <action
        android:id="@+id/action_myMonumentFragment_to_detailFragment"
        app:destination="@id/detailFragment" />

    <include app:graph="@navigation/nav_graph__new_monument_feature" />
</navigation>